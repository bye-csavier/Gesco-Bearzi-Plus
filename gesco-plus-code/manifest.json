{
    "name": "Gesco Plus",
    "version": "1.1.0",
    "description": "Un'estensione che mira a migliorare gesco (gesme-ext)",
    "permissions": ["storage", "tabs","<all_urls>","download"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://gesco.bearzi.it/*"],
        "js": ["contentScript.js"],
        "css": ["contentStyle.css"],
        "run_at": "document_idle"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["*"],
        "matches": ["<all_urls>"]
      }
    ],
    "icons": {
        "16": "assets/img/doggybike.png",
        "24": "assets/img/doggybike.png",
        "32": "assets/img/doggybike.png",
        "48": "assets/img/doggybike.png",
        "128": "assets/img/doggybike.png"
    },
    "action": {
      "default_icon": {
        "16": "assets/img/doggybike.png",
        "24": "assets/img/doggybike.png",
        "32": "assets/img/doggybike.png",
        "48": "assets/img/doggybike.png",
        "128": "assets/img/doggybike.png"
      },
      "default_title": "Medie Gesco",
      "default_popup": "./pop-up/popup.html"
    },
    "manifest_version": 3,
    "options_page":"./gesco-plus/info/gesco-plus.html"
}
